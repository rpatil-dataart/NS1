        <value>(function execute(inputs, outputs) {

  //Transform input to JSON
  var payload = {};

  payload.prefix = inputs.prefix;

  payload.network_id = parseInt(inputs.network_id);

  if(!gs.nil(inputs.desc))   
    payload.desc = inputs.desc;

  if(!gs.nil(inputs.name))   
    payload.name = inputs.name;

  if(!gs.nil(inputs.type))   
    payload.type = inputs.type;

  if(!gs.nil(inputs.kvps)){
       payload.type = JSON.parse(inputs.kvps);
  }

  if(!gs.nil(inputs.tags)){
    payload.tags = JSON.parse(inputs.tags);
  }

  outputs.payload = JSON.stringify(payload);

})(inputs, outputs);</value>
